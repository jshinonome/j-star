  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^24.1.0",
    "apache-arrow": "^21.0.0",
    "babel-jest": "^30.0.5",
    "eslint": "^9.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "esm": "^3.2.25",
    "jest": "^30.0.5",
    "rollup": "^4.46.2",
    "typescript": "^5.9.2"
  }
